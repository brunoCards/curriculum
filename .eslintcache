;\n    }\n  }, []);\n  useEffect(() => {\n    window.localStorage.setItem('emails', JSON.stringify(emails));\n  });\n\n  return (\n    <>\n      <Header>\n        <HeaderTitle className=\"isPersonal\">Dados Pessoais</HeaderTitle>\n      </Header>\n      <PagesContainer>\n        <Main>\n          {emails.length > 0 ? (\n            <Text span=\"Você\" continueContent=\"quer adicionar mais um email?\" />\n          ) : (\n            <Text content=\"Qual o\" span=\"seu\" continueContent=\"melhor email?\" />\n          )}\n          <InputBox>\n            {emails.length > 0 ? (\n              <BackToButton onClick={() => goToReviewPage(history)} />\n            ) : (\n              <BackToButton className=\"isHidden\" />\n            )}\n            <Input\n              className=\"withAdd\"\n              name=\"email\"\n              type=\"email\"\n              value={form.email}\n              onChange={handleOnchangeInput}\n              placeholder=\"Ex: emaildobatman@gothan.com\"\n              title=\"Insira aqui um email válido\"\n              required\n            />\n            {form.email !== '' ? (\n              <AddButton onClick={() => handleAddingEmails(form.email)} />\n            ) : (\n              <AddButton className=\"isdisabled\" />\n            )}\n          </InputBox>\n          <InputBox className=\"email\">\n            <ListBox className=\"email\">\n              {emails.map((email) => {\n                return email.id !== undefined ? (\n                  <ListItem className=\"email\" key={email.id}>\n                    <EmailsIcon onClick={() => handleEditEmail(email.id)} />\n                    {email.text}\n                    <DeleteButton onClick={() => handleDeleteEmail(email.id)} />\n                  </ListItem>\n                ) : (\n                  ''\n                );\n              })}\n            </ListBox>\n          </InputBox>\n        </Main>\n        <MainFooter>\n          <BackButton onClick={() => goBack(history)} />\n          {emails.length > 0 ? (\n            <NextButton onClick={() => goToPhonePage(history)} />\n          ) : (\n            <NextButton\n              onClick={() =>\n                alert(\"Você adicionou um e-mail clicando no botão '+'\")\n              }\n            />\n          )}\n        </MainFooter>\n      </PagesContainer>\n    </>\n  );\n};\nexport default EmailInputPage;\n","/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/CourseNamePage/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/LanguageLevelPage/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/EndDatePage/index.jsx",["219"],"/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/NameInputPage/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/ResignDatePage/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/LandingPage/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/PhoneInputPage/index.jsx",["220"],"/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/KnowledgeInputPage/index.jsx",["221"],"import React, { useEffect } from 'react';\n\n//import context\nimport { useFormulary } from '../../contexts/FormContext';\n\n//import react-hook\nimport useKnowledgeHandler from '../../hooks/useKnowledgeHandler';\n\n//import routers-goTo's\nimport {\n  goToInstitutionPage,\n  goBack,\n  goToReviewPage,\n} from '../../routers/goToPages';\n\n//components\nimport Header from '../../components/Header';\nimport Text from '../../components/Text';\n\n//styled-components\nimport { PagesContainer } from '../../styles/Components/PagesContainer/styles';\nimport { HeaderTitle } from '../../styles/Components/HeaderTitle/styles';\nimport { Main } from '../../styles/Components/MainContainer/styles';\nimport { InputBox } from '../../styles/Components/InputBox/styles';\nimport { Input } from '../../styles/Components/Input/styles';\nimport { CodesIcon } from '../../styles/Components/Icons/styles';\nimport { ListBox, ListItem } from '../../styles/Components/List/styles';\nimport { MainFooter } from '../../styles/Components/Footer/styles';\nimport {\n  BackButton,\n  NextButton,\n  BackToButton,\n  AddButton,\n  DeleteButton,\n} from '../../styles/Components/Buttons/styles';\n\nconst KnowledgeInputPage = () => {\n  const {\n    history,\n    form,\n    handleOnchangeInput,\n    knowledges,\n    setKnowledges,\n  } = useFormulary();\n  const [\n    handleAddingKnowledges,\n    handleEditKnowledge,\n    handleDeleteKnowledge,\n  ] = useKnowledgeHandler();\n\n  useEffect(() => {\n    const persistKnowledges = window.localStorage.getItem('knowledges');\n    if (persistKnowledges) {\n      setKnowledges(JSON.parse(persistKnowledges));\n    }\n  }, []);\n\n  useEffect(() => {\n    window.localStorage.setItem('knowledges', JSON.stringify(knowledges));\n  });\n\n  return (\n    <>\n      <Header>\n        <HeaderTitle className=\"isPersonal\">Dados Pessoais</HeaderTitle>\n      </Header>\n      <PagesContainer>\n        <Main>\n          {knowledges.length > 0 ? (\n            <Text\n              content=\"mais algum\"\n              span=\"conhecimento\"\n              continueContent=\"?\"\n            />\n          ) : (\n            <Text\n              content=\"Adicione aqui os\"\n              span=\"seus\"\n              continueContent=\"conhecimentos\"\n            />\n          )}\n          <InputBox>\n            {knowledges.length > 0 ? (\n              <BackToButton onClick={() => goToReviewPage(history)} />\n            ) : (\n              <BackToButton className=\"isHidden\" />\n            )}\n            <Input\n              className=\"withAdd\"\n              name=\"knowledge\"\n              value={form.knowledge}\n              onChange={handleOnchangeInput}\n              placeholder=\"Ex: HTML\"\n            />\n            {form.knowledge !== '' ? (\n              <AddButton\n                onClick={() => handleAddingKnowledges(form.knowledge)}\n              />\n            ) : (\n              <AddButton className=\"isdisabled\" aria-disabled=\"true\" />\n            )}\n          </InputBox>\n          <InputBox className=\"knowledge\">\n            <ListBox className=\"knowledge\">\n              {knowledges.map((knowledge) => {\n                return (\n                  <ListItem className=\"knowledge\" key={knowledge.id}>\n                    <CodesIcon\n                      onClick={() => handleEditKnowledge(knowledge.id)}\n                    />\n                    {knowledge.text}\n                    <DeleteButton\n                      onClick={() => handleDeleteKnowledge(knowledge.id)}\n                    />\n                  </ListItem>\n                );\n              })}\n            </ListBox>\n          </InputBox>\n        </Main>\n        <MainFooter>\n          <BackButton onClick={() => goBack(history)} />\n          {knowledges.length > 0 ? (\n            <NextButton onClick={() => goToInstitutionPage(history)} />\n          ) : (\n            <NextButton\n              onClick={() =>\n                alert(\n                  \"Adicione pelo menos 1 conhecimento clicando no botão '+'\"\n                )\n              }\n            />\n          )}\n        </MainFooter>\n      </PagesContainer>\n    </>\n  );\n};\n\nexport default KnowledgeInputPage;\n","/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/StartDatePage/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/ReviewPage/index.jsx",["222","223"],"import React from 'react';\n\n//import routers-goTo's\nimport {\n  goToNamePage,\n  goToStateInputPage,\n  goToPhonePage,\n  goToEmailPage,\n  goToDescriptionPage,\n  goToInstitutionPage,\n  goToCompanyNamePage,\n} from '../../routers/goToPages';\n\n//import context\nimport { useFormulary } from '../../contexts/FormContext';\n\n//ownStyles\nimport {\n  ReviewPageLayout,\n  PersonalDataSection,\n  FullName,\n  AddressBox,\n  AddressContainer,\n  Address,\n  Contacts,\n  ContactsBox,\n  PinIcon,\n  PhoneIcon,\n  EmailIcon,\n  ContactsText,\n  ResumeBox,\n  SectionsTitle,\n  SectionsParagraph,\n  AcademicData,\n  AcademicBox,\n  BoxSubtitle,\n  KnowledgesData,\n  ProffesionalData,\n  SectionBox,\n} from './styles';\n\n//components\nimport Header from '../../components/Header';\n\n//styled-components\nimport { ListBox, ListItem } from '../../styles/Components/List/styles';\n\nconst ReviewPage = () => {\n  const { form, address, emails, phones, knowledges, history } = useFormulary();\n\n  return (\n    <>\n      <ReviewPageLayout>\n        <PersonalDataSection>\n          <FullName onClick={() => goToNamePage(history)}>{form.name}</FullName>\n          <AddressContainer>\n            <AddressBox onClick={() => goToStateInputPage(history)}>\n              <PinIcon />\n              <Address>\n                {address.localidade + ' - '}\n                {address.uf}\n              </Address>\n            </AddressBox>\n          </AddressContainer>\n          <Contacts>\n            <ContactsBox>\n              {phones.map((phone) => {\n                return (\n                  <ContactsText\n                    onClick={() => goToPhonePage(history)}\n                    className=\"phone\"\n                    key={phone.id}\n                  >\n                    <PhoneIcon />\n                    {phone.number}\n                  </ContactsText>\n                );\n              })}\n            </ContactsBox>\n            <ContactsBox>\n              {emails.map((email) => {\n                return (\n                  <ContactsText\n                    onClick={() => goToEmailPage(history)}\n                    className=\"email\"\n                    key={email.id}\n                  >\n                    <EmailIcon />\n                    {email.text}\n                  </ContactsText>\n                );\n              })}\n            </ContactsBox>\n            <ResumeBox>\n              <SectionsTitle\n                onClick={() => goToDescriptionPage(history)}\n                className=\"resume\"\n              >\n                Resumo\n              </SectionsTitle>\n              <SectionsParagraph className=\"resume\">\n                {form.description}\n              </SectionsParagraph>\n            </ResumeBox>\n          </Contacts>\n        </PersonalDataSection>\n        <AcademicData>\n          <SectionsTitle\n            onClick={() => goToInstitutionPage(history)}\n            className=\"academicSectionTitle\"\n          >\n            Informações Acadêmicas\n          </SectionsTitle>\n          <AcademicBox>\n            <BoxSubtitle className=\"course\">{form.institution}</BoxSubtitle>\n            <BoxSubtitle className=\"fontSizeAdjust\">\n              {form.degree} {form.course}\n            </BoxSubtitle>\n            <SectionsParagraph>\n              Curso de 6 meses de duração, com 1000 horas de estudos em aulas,\n              exercícios e projetos práticos, o curso me trouxe até meu nível de\n              aprendizado atual. Aprendi Javascript, React, Node JS, consumo e\n              criação de API’S REST (Express) , uso de banco de dados utilizando\n              a linguagem mySQL.\n            </SectionsParagraph>\n            <SectionsParagraph>\n              Início em {form.startdate} até {form.enddate}\n            </SectionsParagraph>\n\n            <BoxSubtitle className=\"course\">{form.institution}</BoxSubtitle>\n            <BoxSubtitle className=\"fontSizeAdjust\">\n              {form.degree} {form.course}\n            </BoxSubtitle>\n            <SectionsParagraph>\n              Curso de 6 meses de duração, com 1000 horas de estudos em aulas,\n              exercícios e projetos práticos, o curso me trouxe até meu nível de\n              aprendizado atual. Aprendi Javascript, React, Node JS, consumo e\n              criação de API’S REST (Express) , uso de banco de dados utilizando\n              a linguagem mySQL.\n            </SectionsParagraph>\n            <SectionsParagraph>\n              Início em {form.startdate} até {form.enddate}\n            </SectionsParagraph>\n          </AcademicBox>\n        </AcademicData>\n        <KnowledgesData>\n          <SectionsTitle className=\"KnowledgesSectionTitle\">\n            Qualificações e Idiomas\n          </SectionsTitle>\n          <SectionBox>\n            <ListBox className=\"knowledges\">\n              {knowledges.map((knowledge) => {\n                return <ListItem key={knowledge.id}>{knowledge.text}</ListItem>;\n              })}\n            </ListBox>\n            <ListBox className=\"languages\">\n              <ListItem>{form.language}</ListItem>\n              <ListItem>{form.level}</ListItem>\n            </ListBox>\n          </SectionBox>\n        </KnowledgesData>\n        <ProffesionalData>\n          <SectionsTitle className=\"proffesionalSectionTitle\">\n            Experiências Profisisonais\n          </SectionsTitle>\n          <BoxSubtitle className=\"company\">{form.company}</BoxSubtitle>\n          <SectionsParagraph className=\"company\">\n            {form.activities}\n          </SectionsParagraph>\n          <SectionsParagraph className=\"company\">\n            Início em {form.admitdate} até {form.resigndate}\n          </SectionsParagraph>\n        </ProffesionalData>\n      </ReviewPageLayout>\n    </>\n  );\n};\n\nexport default ReviewPage;\n","/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/EducationalInstitutionPage/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/hooks/useForm.js",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/routers/goToPages.js",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/styles/Components/Footer/styles.js",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/styles/Components/PagesContainer/styles.js",[],"/home/brunocardoso/D