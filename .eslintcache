>\n            <BackToButton\n              onClick={() => goToReviewPage(history)}\n              className=\"state\"\n            />\n            <Select>\n              {cities.map((city) => {\n                return selectedCity === null ? (\n                  <SelectOption\n                    key={city.id}\n                    onClick={handleOnOptionClicked(city)}\n                  >\n                    {city.nome}\n                  </SelectOption>\n                ) : (\n                  <SelectOption\n                    style={{\n                      backgroundColor:\n                        selectedCity.id === city.id ? 'var(--purple)' : '',\n                    }}\n                    key={city.id}\n                    onClick={handleOnOptionClicked(city)}\n                  >\n                    {city.nome}\n                  </SelectOption>\n                );\n              })}\n            </Select>\n            <OpenSelect onClick={handleToggle} />\n          </InputBox>\n        </Main>\n        <MainFooter>\n          <BackButton onClick={() => goBack(history)} />\n          <NextButton onClick={() => goToEmailPage(history)} />\n        </MainFooter>\n      </PagesContainer>\n    </>\n  );\n};\nexport default CityInputPage;\n","/home/brunocardoso/Documentos/Projetos/curriculum/src/components/Input/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/components/Input/styles.js",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/utils/handlePhoneFormat.js",[],{"ruleId":"243","replacedBy":"244"},{"ruleId":"245","replacedBy":"246"},{"ruleId":"247","severity":1,"message":"248","line":53,"column":6,"nodeType":"249","endLine":53,"endColumn":8,"suggestions":"250"},{"ruleId":"251","severity":1,"message":"252","line":23,"column":10,"nodeType":"253","messageId":"254","endLine":23,"endColumn":15},{"ruleId":"247","severity":1,"message":"255","line":60,"column":6,"nodeType":"249","endLine":60,"endColumn":8,"suggestions":"256"},{"rule