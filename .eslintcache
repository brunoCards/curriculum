Handler.js",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/hooks/useEmailHandler.js",["230","231","232","233","234","235","236","237","238","239"],"//import context\nimport { useFormulary } from '../contexts/FormContext';\n\nconst useEmailHandler = () => {\n  const { emails, setEmails, form, setForm } = useFormulary();\n\n  //Emailhandlers\n  const handleAddingEmails = () => {\n    const newEmail = {\n      id: new Date().getTime(),\n      text: form.email,\n    };\n    let pattern = /^(([^<>()[\\]\\.,;:\\s@\\\"]+(\\.[^<>()[\\]\\.,;:\\s@\\\"]+)*)|(\\\".+\\\"))@(([^<>()[\\]\\.,;:\\s@\\\"]+\\.)+[^<>()[\\]\\.,;:\\s@\\\"]{2,})$/;\n    const patternTest = pattern.test(newEmail.text);\n    if (!newEmail || patternTest === false) {\n      alert('Insira um email válido!');\n      return;\n    }\n    if (\n      emails.find(\n        (email) => email.text.toLowerCase() === form.email.toLowerCase()\n      )\n    ) {\n      alert('Este email já está em uso');\n      return;\n    }\n    setEmails([...emails].concat(newEmail));\n    setForm({ ...form, email: '' });\n  };\n\n  const handleDeleteEmail = (id) => {\n    setEmails(emails.filter((email) => email.id !== id));\n  };\n\n  const handleEditEmail = (id) => {\n    if (form.email !== '') {\n      alert('Você já está editando um email');\n      return;\n    }\n    const findEmail = emails.find((email) => email.id === id);\n    if (findEmail !== -1) {\n      emails.pop(findEmail, 1);\n      setForm({ ...form, email: findEmail.text });\n    }\n  };\n  return [handleAddingEmails, handleEditEmail, handleDeleteEmail];\n};\nexport default useEmailHandler;\n","/home/brunocardoso/Documentos/Projetos/curriculum/src/styles/Components/Icons/styles.js",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/services/api.js",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/components/Select/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/components/Select/styles.js",[],"/home/brunocardoso/Document