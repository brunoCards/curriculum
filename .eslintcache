./styles/Components/List/styles';\nimport { EmailsIcon } from '../../styles/Components/Icons/styles';\nimport { MainFooter } from '../../styles/Components/Footer/styles';\nimport { HeaderTitle } from '../../styles/Components/HeaderTitle/styles';\nimport {\n  BackButton,\n  NextButton,\n  BackToButton,\n  AddButton,\n  DeleteButton,\n} from '../../styles/Components/Buttons/styles';\n\nconst EmailInputPage = () => {\n  const {\n    history,\n    form,\n    handleOnchangeInput,\n    emails,\n    setEmails,\n  } = useFormulary();\n\n  const [\n    handleAddingEmails,\n    handleEditEmail,\n    handleDeleteEmail,\n  ] = useEmailHandler();\n\n  useEffect(() => {\n    const persistEmails = window.localStorage.getItem('emails');\n    if (persistEmails) {\n      setEmails(JSON.parse(persistEmails));\n    }\n  }, []);\n  useEffect(() => {\n    window.localStorage.setItem('emails', JSON.stringify(emails));\n  });\n\n  return (\n    <>\n      <Header>\n        <HeaderTitle className=\"isPersonal\">Dados Pessoais</HeaderTitle>\n      </Header>\n      <PagesContainer>\n        <Main>\n          {emails.length > 0 ? (\n            <Text span=\"Você\" continueContent=\"quer adicionar mais um email?\" />\n          ) : (\n            <Text content=\"Qual o\" span=\"seu\" continueContent=\"melhor email?\" />\n          )}\n          <InputBox>\n            {emails.length > 0 ? (\n              <BackToButton onClick={() => goToReviewPage(history)} />\n            ) : (\n              <BackToButton className=\"isHidden\" />\n            )}\n            <Input\n              className=\"withAdd\"\n              name=\"email\"\n              type=\"email\"\n              value={form.email}\n              onChange={handleOnchangeInput}\n              placeholder=\"Ex: emaildobatman@gothan.com\"\n              title=\"Insira aqui um email válido\"\n              required\n            />\n            {form.email !== '' ? (\n              <AddButton onClick={() => handleAddingEmails(form.email)} />\n            ) : (\n              <AddButton className=\"isdisabled\" />\n            )}\n          </InputBox>\n          <InputBox className=\"email\">\n            <ListBox className=\"email\">\n              {emails.map((email) => {\n                return email.id !== undefined ? (\n                  <ListItem className=\"email\" key={email.id}>\n                    <EmailsIcon onClick={() => handleEditEmail(email.id)} />\n                    {email.text}\n                    <DeleteButton onClick={() => handleDeleteEmail(email.id)} />\n                  </ListItem>\n                ) : (\n                  ''\n                );\n              })}\n            </ListBox>\n          </InputBox>\n        </Main>\n        <MainFooter>\n          <BackButton onClick={() => goBack(history)} />\n          {emails.length > 0 ? (\n            <NextButton onClick={() => goToPhonePage(history)} />\n          ) : (\n            <NextButton\n              onClick={() =>\n                alert(\"Você adicionou um e-mail clicando no botão '+'\")\n              }\n            />\n          )}\n        </MainFooter>\n      </PagesContainer>\n    </>\n  );\n};\nexport default EmailInputPage;\n","/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/CourseNamePage/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/LanguageLevelPage/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/EndDatePage/index.jsx",["227"],"import React from 'react';\n\n//import context\nimport { useFormulary } from '../../contexts/FormContext';\n\n//import routers-goTo's\nimport {\n  goToLanguagesPage,\n  goBack,\n  goToReviewPage,\n} from '../../routers/goToPages';\n\n//components\nimport Header from '../../components/Header';\nimport Text from '../../components/Text';\nimport InputComponent from '../../components/Input';\n\n//styled-components\nimport { PagesContainer } from '../../styles/Components/PagesContainer/styles';\nimport { HeaderTitle } from '../../styles/Components/HeaderTitle/styles';\nimport { Main } from '../../styles/Components/MainContainer/styles';\nimport { InputBox } from '../../styles/Components/InputBox/styles';\nimport { Input } from '../../components/Input/styles';\nimport { MainFooter } from '../../styles/Components/Footer/styles';\nimport {\n  BackButton,\n  NextButton,\n  BackToButton,\n} from '../../styles/Components/Buttons/styles';\n\nconst EndDatePage = () => {\n  const { history, form, handleOnchangeInput } = useFormulary();\n\n  return (\n    <>\n      <Header>\n        <HeaderTitle className=\"isAcademic\">Dados Acadêmicos</HeaderTitle>\n      </Header>\n      <PagesContainer>\n        <Main>\n          <Text\n            content=\"Qual a data de\"\n            span=\"término\"\n            continueContent=\" do curso?\"\n          />\n          <InputBox>\n            {form.enddate !== '' ? (\n              <BackToButton onClick={() => goToReviewPage(history)} />\n            ) : (\n              <BackToButton className=\"isHidden\" />\n            )}\n            <InputComponent\n              className=\"withoutAdd\"\n              name=\"enddate\"\n              mask=\"aaa/9999\"\n              value={form.enddate}\n              onChange={handleOnchangeInput}\n              placeholder={'Ex: jul/2020'}\n            />\n          </InputBox>\n        </Main>\n        <MainFooter>\n          <BackButton onClick={() => goBack(history)} />\n          <NextButton onClick={() => goToLanguagesPage(history)} />\n        </MainFooter>\n      </PagesContainer>\n    </>\n  );\n};\n\nexport default EndDatePage;\n","/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/NameInputPage/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/ResignDatePage/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/LandingPage/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/screens/PhoneInputPage/index.jsx",["