ty = window.localStorage.getItem('selectedCity');\n    if (persistSelectedCity) {\n      setSelectedCity(JSON.parse(persistSelectedCity));\n    }\n  }, []);\n\n  useEffect(() => {\n    window.localStorage.setItem('selectedCity', JSON.stringify(selectedCity));\n  });\n\n  return (\n    <>\n      <Header>\n        <HeaderTitle className=\"isPersonal\">Dados Pessoais</HeaderTitle>\n      </Header>\n      <PagesContainer>\n        <Main>\n          <Text content=\"Informe também a Cidade\" />\n          <InputBox>\n            <BackToButton\n              onClick={() => goToReviewPage(history)}\n              className=\"state\"\n            />\n            <Select>\n              {cities.map((city) => {\n                return selectedCity === null ? (\n                  <SelectOption\n                    key={city.id}\n                    onClick={handleOnOptionClicked(city)}\n                  >\n                    {city.nome}\n                  </SelectOption>\n                ) : (\n                  <SelectOption\n                    style={{\n                      backgroundColor:\n                        selectedCity.id === city.id ? 'var(--purple)' : '',\n                    }}\n                    key={city.id}\n                    onClick={handleOnOptionClicked(city)}\n                  >\n                    {city.nome}\n                  </SelectOption>\n                );\n              })}\n            </Select>\n            <OpenSelect onClick={handleToggle} />\n          </InputBox>\n        </Main>\n        <MainFooter>\n          <BackButton onClick={() => goBack(history)} />\n          <NextButton onClick={() => goToEmailPage(history)} />\n        </MainFooter>\n      </PagesContainer>\n    </>\n  );\n};\nexport default CityInputPage;\n","/home/brunocardoso/Documentos/Projetos/curriculum/src/components/Input/index.jsx",[],"/home/brunocardoso/Documentos/Projetos/curriculum/src/components/Input/styles.js",[],"/home/brunocardoso/Documentos/Projetos/cur